.nav-l, .nav-r, .nav-title {
  position: relative;
  width: 66rpx;
  height: 100%;
  text-align: center;
  line-height: 66rpx;
  font-size: 21.6rpx;
}

.nav {
  display: flex;
  width: 100%;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 38.4rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.4rpx;
  -moz-osx-font-smoothing: grayscale;
  color: #000;
}

.icon-set:before {
  content: "\e74f";
}

.nav-t {
  top: 0;
  height: 66rpx;
  position: fixed;
  z-index: 100;
}

.nav-bgc {
  background: #fff;
  box-shadow: 0 2rpx 4rpx #ccc;
}

.icon-l:before {
  content: "\e743";
}

.nav-title {
  color: #444;
  font-size: 27.6rpx;
  flex: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.my {
  margin: 0;
  padding: 0;
  border: 0;
  font: inherit;
  vertical-align: baseline;
}
