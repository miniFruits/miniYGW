@font-face {
  font-family: 'iconfont'; /* project id 1600305 */
  src: url('//at.alicdn.com/t/font_1600305_ze6ww9hcfs.eot');
  src: url('//at.alicdn.com/t/font_1600305_ze6ww9hcfs.eot?#iefix') format('embedded-opentype'),
         url('//at.alicdn.com/t/font_1600305_ze6ww9hcfs.woff2') format('woff2'),
         url('//at.alicdn.com/t/font_1600305_ze6ww9hcfs.woff') format('woff'),
         url('//at.alicdn.com/t/font_1600305_ze6ww9hcfs.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_1600305_ze6ww9hcfs.svg#iconfont') format('svg');
}

page {
  height: 100%;
  width: 100%;
  background: #f0f2f5;
}

.user {
  height: 1000%;
  width: 100%;
}

.nav {
  display: flex;
  width: 100%;
}

.nav-bgc {
  background: #fff;
  box-shadow: 0 1px 2px #ccc;
}

.none {
  opacity: 0;
}

.nav-t {
  top: 0;
  height: 66rpx;
  position: fixed;
  z-index: 100;
}

.nav-l, .nav-r, .nav-title {
  position: relative;
  width: 66rpx;
  height: 100%;
  text-align: center;
  line-height: 66rpx;
  font-size: 21.6rpx;
}

.nav-img {
  height: 70%;
  border-radius: 50%;
  max-width: 100%;
  vertical-align: middle;
}

.nav-title {
  color: #444;
  font-size: 27.6rpx;
  flex: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.my {
  margin: 0;
  padding: 0;
  border: 0;
  font: inherit;
  vertical-align: baseline;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 38.4rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.4rpx;
  -moz-osx-font-smoothing: grayscale;
  color: #fff;
}

.icon-set:before {
  content: "\e61a";
}

.content-nt {
  width: 100%;
}

.content-nt:after {
  content: '';
  display: block;
  width: 100%;
  height: 93.6rpx;
}

.user-wrap {
  width: 100%;
  height: 229.25rpx;
  min-height: 229.25rpx;
  position: relative;
}

.card-view {
  margin: 3.206%;
  width: 93.588%;
  background-color: #fafafa;
  box-shadow: 0 4rpx 4rpx rgba(207, 207, 207, 0.3);
  background: #fff;
  border-radius: 4.8rpx;
}

.flx {
  display: -webkit-flex;
}

.bg {
  width: 100%;
  max-width: 100%;
  height: 100%;
  vertical-align: middle;
}

.user-ht {
  width: 100%;
  height: 107.592rpx;
  position: absolute;
  top: calc(55% - 53.769rpx);
  left: 0;
  padding-left: 31.4484rpx;
}

.us {
  height: 107.592rpx;
  width: 107.592rpx;
  border-radius: 50%;
  background: #fff;
  max-width: 100%;
  vertical-align: middle;
}

.user-t {
  color: #fff;
  font-size: 20.16rpx;
  line-height: 31.2rpx;
  padding: 0 28.1376rpx;
}

.flx-l {
  flex: 1;
}

.greet {
  padding-top: 31.2rpx;
  font-size: 23.04rpx;
}

.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.img {
  width: 20%;
  min-height: 105rpx;
  padding: 12.414rpx 0;
  text-align: center;
}

.order {
  width: 67.872rpx;
  height: 67.872rpx;
  max-width: 100%;
  vertical-align: middle;
}

.item-name {
  color: #282828;
  font-size: 19.68rpx;
  line-height: 36rpx;
}
